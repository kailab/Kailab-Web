<script type="text/javascript">
$(document).ready(function(){
    var options = {
        zoom: 14,
        center: new google.maps.LatLng(41.391868, 2.168099),
        mapTypeId: google.maps.MapTypeId.ROADMAP
    };
    var map = new google.maps.Map($('#map_canvas').get(0), options);

